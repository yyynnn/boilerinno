{
    "name": "boilerinno",
    "description": "boilerinno boilerplate",
    "private": true,
    "scripts": {
        "dev": "node ./scripts/dev-menu.js",
        "build": "cross-env NODE_ENV=production webpack --config ./packages/root-app/webpack/webpack.prod.babel.js",
        "build:serve": "npm run build && npx http-server -p 7777 ./dist",
        "lint": "eslint . --ext .ts,.tsx --fix-dry-run",
        "lint:staged": "lint-staged",
        "test": "echo \"Error: no test specified\" && exit 1",
        "cm": "npx git-cz",
        "cm:retry": "npx git-cz --retry",
        "registry:local:setup": "npm config set strict-ssl false -g && npm add -g verdaccio pm2",
        "registry:local:verdaccio": "verdaccio",
        "registry:local:run": "pm2 start npm --name verdaccio -- run registry:local:verdaccio && echo 'local registry web ui - http://localhost:4873'",
        "_postinstall": "npm run build:packages:yarn",
        "release:main": "npm run build:packages:yarn && cross-env GL_TOKEN=$GITLAB_TOKEN NODE_TLS_REJECT_UNAUTHORIZED=$NODE_TLS_REJECT_UNAUTHORIZED PROJECT_REPO_URL=$PROJECT_REPO_URL REPO_HOST_URL=$REPO_HOST_URL CI_COMMIT_REF_NAME=$CI_COMMIT_REF_NAME multi-semantic-release --sequential-init"
    },
    "workspaces": [
        "packages/*"
    ],
    "author": "",
    "license": "ISC",
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": "./.cz-config.js"
        }
    },
    "engines": {
        "node": ">=14.2.0 <=16.5.0"
    },
    "devDependencies": {
        "@babel/core": "7.14.2",
        "@babel/eslint-parser": "7.14.3",
        "@babel/plugin-proposal-class-properties": "7.13.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.2",
        "@babel/plugin-proposal-optional-chaining": "7.14.2",
        "@babel/plugin-syntax-dynamic-import": "7.8.3",
        "babel-plugin-styled-components": "1.13.2",
        "@babel/preset-env": "7.14.2",
        "@babel/preset-react": "7.13.13",
        "@babel/preset-typescript": "7.13.0",
        "@babel/register": "7.13.16",
        "@commitlint/cli": "11.0.0",
        "@commitlint/config-angular": "11.0.0",
        "@commitlint/config-conventional": "11.0.0",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/commit-analyzer": "8.0.1",
        "@semantic-release/exec": "5.0.0",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/gitlab": "6.0.5",
        "@semantic-release/npm": "7.0.6",
        "@semantic-release/release-notes-generator": "9.0.1",
        "@types/history": "4.7.9",
        "@types/lodash-es": "4.17.3",
        "@types/axios": "0.14.0",
        "@types/react": "17.0.15",
        "@types/react-beautiful-dnd": "13.0.0",
        "@types/react-custom-scrollbars": "4.0.6",
        "@types/react-datepicker": "3.1.1",
        "@types/react-dom": "17.0.9",
        "@types/react-router": "5.1.16",
        "@types/react-router-dom": "5.1.8",
        "@types/react-select": "3.0.21",
        "@types/recharts": "1.8.7",
        "@types/styled-components": "5.1.11",
        "@types/superagent": "4.1.4",
        "@types/terser-webpack-plugin": "4.2.0",
        "@babel/eslint-plugin": "7.13.16",
        "@typescript-eslint/eslint-plugin": "4.1.0",
        "@typescript-eslint/parser": "4.3.0",
        "confusing-browser-globals": "1.0.9",
        "eslint-import-resolver-webpack": "0.13.0",
        "eslint-import-resolver-typescript": "2.3.0",
        "eslint-plugin-mdx": "1.8.2",
        "eslint-plugin-no-use-extend-native": "0.5.0",
        "eslint-plugin-sort-destructure-keys": "1.3.5",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-json": "2.1.2",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-react": "7.21.5",
        "eslint-plugin-react-hooks": "4.2.0",
        "eslint-plugin-functional": "3.2.1",
        "commitizen": "4.2.1",
        "cross-env": "7.0.2",
        "cz-conventional-changelog": "3.3.0",
        "cz-customizable": "6.3.0",
        "dotenv": "8.2.0",
        "eslint": "7.10.0",
        "execa": "4.1.0",
        "fast-glob": "3.2.4",
        "fs-extra": "9.0.1",
        "git-branch": "2.0.1",
        "git-revision-webpack-plugin": "3.0.6",
        "http-server": "0.12.3",
        "husky": "4.3.0",
        "json-server": "0.16.3",
        "lint-staged": "10.4.0",
        "multi-semantic-release": "2.8.2",
        "prettier": "2.1.2",
        "prompts": "2.4.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "semantic-release": "17.4.3",
        "tsconfig-paths-webpack-plugin": "3.5.1",
        "typescript": "4.3.5",
        "webpack-bundle-size-analyzer": "3.1.0"
    },
    "version": "1.0.0"
}
